    "build": "npm run clean && tsup && npm run build:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zustin-lu/commerce-ui.git"
  },
  "author": "Justin Lu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zustin-lu/commerce-ui/issues"
  },
  "homepage": "https://github.com/zustin-lu/commerce-ui#readme",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.3",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/react": "^18.3.3",
    "agadoo": "^3.0.0",
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.2",
    "husky": "^9.0.11",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.7",
    "tailwindcss": "^3.4.4",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.395.0",
    "tailwind-merge": "^2.3.0",
