{
  "name": "@justinlu/commerce-ui",
  "version": "1.0.0",
  "description": "UI components for using across ecommerce project. Currently it basically use inside OrderHQ projects.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "prepare": "husky",
